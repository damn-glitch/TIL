{
  "Main Function": {
    "prefix": "main",
    "body": [
      "main()",
      "    $0"
    ],
    "description": "Main entry point"
  },
  "Function": {
    "prefix": "fn",
    "body": [
      "${1:name}(${2:params}) -> ${3:type}",
      "    $0"
    ],
    "description": "Function definition"
  },
  "Struct": {
    "prefix": "struct",
    "body": [
      "struct ${1:Name}",
      "    ${2:field}: ${3:type}",
      "$0"
    ],
    "description": "Struct definition"
  },
  "Impl": {
    "prefix": "impl",
    "body": [
      "impl ${1:Type}",
      "    $0"
    ],
    "description": "Impl block"
  },
  "Enum": {
    "prefix": "enum",
    "body": [
      "enum ${1:Name}",
      "    ${2:Variant}",
      "$0"
    ],
    "description": "Enum definition"
  },
  "If": {
    "prefix": "if",
    "body": [
      "if ${1:condition}",
      "    $0"
    ],
    "description": "If statement"
  },
  "If-Else": {
    "prefix": "ife",
    "body": [
      "if ${1:condition}",
      "    $2",
      "else",
      "    $0"
    ],
    "description": "If-else statement"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for ${1:i} in ${2:0..10}",
      "    $0"
    ],
    "description": "For loop"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while ${1:condition}",
      "    $0"
    ],
    "description": "While loop"
  },
  "Loop": {
    "prefix": "loop",
    "body": [
      "loop",
      "    $0"
    ],
    "description": "Infinite loop"
  },
  "Let": {
    "prefix": "let",
    "body": "let ${1:name} = ${2:value}$0",
    "description": "Immutable variable"
  },
  "Var": {
    "prefix": "var",
    "body": "var ${1:name} = ${2:value}$0",
    "description": "Mutable variable"
  },
  "Print": {
    "prefix": "print",
    "body": "print(${1:value})$0",
    "description": "Print statement"
  },
  "Level 0": {
    "prefix": "level0",
    "body": [
      "#[level: 0]",
      "${1:name}(${2:params}) -> ${3:type}",
      "    $0"
    ],
    "description": "Level 0 function (Hardware)"
  },
  "Level 1": {
    "prefix": "level1",
    "body": [
      "#[level: 1]",
      "${1:name}(${2:params}) -> ${3:type}",
      "    $0"
    ],
    "description": "Level 1 function (Systems)"
  },
  "Level 3": {
    "prefix": "level3",
    "body": [
      "#[level: 3]",
      "${1:name}(${2:params})",
      "    $0"
    ],
    "description": "Level 3 function (Script)"
  },
  "Constructor": {
    "prefix": "new",
    "body": [
      "new(${1:params}) -> ${2:Self}",
      "    return ${2:Self} { $0 }"
    ],
    "description": "Constructor method"
  }
}
