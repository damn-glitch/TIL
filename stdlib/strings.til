# TIL Standard Library: Strings
# String utility functions

repeat(s: str, n: int) -> str
    var result = ""
    for i in 0..n
        result = result + s
    return result

is_empty(s: str) -> bool
    return len(s) == 0

reverse_str(s: str) -> str
    let n = len(s)
    var result = ""
    var i = n - 1
    while i >= 0
        result = result + s.slice(i, i + 1)
        i -= 1
    return result
